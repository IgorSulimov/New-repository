using namespace std;
class Analyzator
{
private:
	ofstream file_error;
	string str;
	typedef void(Analyzator::* pfuncType)();// указатель на функцию состояния
	pfuncType pfunc;
	int RCH, // регистр числа
		RZ, // регистр знака
